{
  "source": "./src",
  "destination": "./engg4030",
  "metadata": {
    "title": "Tutorial Notes on Web-Scale Information Analytics",
    "description": "Archived from course CUHK-ENGG4030"
  },
  "plugins": {
    "metalsmith-metadata": {
        "site": "site.yaml"
    },
    "metalsmith-drafts": {},
    "metalsmith-markdown": {},
    "metalsmith-headings": "h2",
    "metalsmith-debug":{
        "enabled": false
    },
    "metalsmith-templates": {
      "engine": "swig",
      "directory": "templates"
    },
    "metalsmith-assets": {
      "source": "./assets",
      "destination": "./assets"
    },
    "metalsmith-sass": {
      "outputStyle": "expanded"
    }
  }
}
